<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="Localization\DynamicPlugin\en.json" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Pages\DynamicPlugin\Index.js" />
    <None Remove="Pages\DynamicPlugin\Upload.js" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\DynamicPlugin\en.json" />
    <EmbeddedResource Include="Pages\DynamicPlugin\Index.js" />
    <EmbeddedResource Include="Pages\DynamicPlugin\Upload.js" />
  </ItemGroup>

  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="8.0.4" />
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
    <PackageReference Include="Serilog.Extensions.Hosting" Version="8.0.0" />
    <PackageReference Include="Volo.Abp.AspNetCore" Version="8.1.3" />
    <PackageReference Include="Volo.Abp.AspNetCore.Mvc.UI.Theme.Shared" Version="8.1.3" />
    <PackageReference Include="Volo.Abp.AutoMapper" Version="8.1.3" />
    <PackageReference Include="Volo.Abp.BlobStoring.FileSystem" Version="8.1.3" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\" />
  </ItemGroup>

</Project>
